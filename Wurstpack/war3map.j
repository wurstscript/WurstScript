        TriggerRegisterPlayerUnitEvent( trig, ENEMY, EVENT_PLAYER_UNIT_DEATH, null )
        TriggerRegisterPlayerUnitEvent( trig, Player( 0 ), EVENT_PLAYER_UNIT_DEATH, null )
